{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "arrow-parens": [false],
    "no-default-export": true,
    "no-console": [false],
    "semicolon": [true, "never"],
    "interface-name": [false],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "ignore"
        }
      }
    ]
  }
}
